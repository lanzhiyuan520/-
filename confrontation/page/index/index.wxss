.container {
    padding-top: 280rpx;
}

.intro {
    height: 100%;
    text-align: center;
    font-size: 0;
}

.title {
    margin-top: 121rpx;
    width: 552rpx;
    height: 132rpx;
}

.playChoose {
    display: flex;
    align-items: center;
    justify-content: center;
    margin-bottom: 162rpx;
    margin-top: 100rpx;
    font-size: 0;
}

.play1 {
    margin-right: 18rpx;
}

.play1,.play2 {
    width: 290rpx;
    height: 400rpx;
}

.play1.playActive,.play2.playActive {
    border: 2rpx solid #fff;
    border-radius: 12rpx;
}

.homeBtns {
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 0;
}

.homeShare {
    margin: 0;
    background: url(data:image/png;base64,) 50% no-repeat;
    background-size: 252rpx 98rpx;
}

.homeShare,.introStart {
    width: 252rpx;
    height: 98rpx;
}

.introStart {
    margin-left: 28rpx;
    background: url(data:image/png;base64,) 50% no-repeat;
    background-size: 252rpx 98rpx;
}

.box,.main {
    height: 100%;
    box-sizing: border-box;
}

.avatarList {
    position: relative;
    width: 90%;
    height: 60%;
    margin: 0 auto;
    font-size: 0;
    border-radius: 12rpx;
}

.avatarItem {
    position: absolute;
    z-index: 1;
}

.result {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    z-index: 4;
    font-size: 0;
}

.result image {
    width: 402rpx;
    height: 450rpx;
}

.muiscIcon {
    position: absolute;
    top: 59rpx;
    right: 5%;
    width: 40rpx;
    height: 40rpx;
    background: url(https://img.cardcute.com/smd/newMuiscIcon.png) 50% no-repeat;
    background-size: 40rpx 40rpx;
    z-index: 3;
    animation: muiscFrames 3s linear infinite;
}

@-webkit-keyframes muiscFrames {
    0% {
        transform: rotate(0deg);
    }

    to {
        transform: rotate(1turn);
    }
}

@keyframes muiscFrames {
    0% {
        transform: rotate(0deg);
    }

    to {
        transform: rotate(1turn);
    }
}

.muiscIcon.no {
    background: url(https://img.cardcute.com/smd/newMuiscIconNo.png) 50% no-repeat;
    background-size: 40rpx 40rpx;
    animation: none;
}

.active {
    transition: all .6s ease-out;
    transform: translateY(-2000rpx) rotate(-90deg);
}

.btns {
    z-index: 100;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 84%;
    margin: 68rpx auto 0;
}

.btn {
    width: 252rpx;
    height: 98rpx;
    line-height: 98rpx;
    margin: 0;
    background-color: #bc3132;
    border-radius: 10rpx;
    color: #fff;
    font-weight: 700;
}

.btn,.tips {
    text-align: center;
}

.tips {
    padding-top: 30rpx;
    margin-bottom: 45rpx;
    font-size: 30rpx;
    color: #fba922;
    opacity: .6;
}

.shaizi-box {
    position: relative;
    width: 90%;
    margin: 0 auto;
    border-radius: 12rpx;
    overflow: hidden;
}

.sz-box {
    position: relative;
    height: 300rpx;
}

.sz-resultImg {
    position: absolute;
    right: 10%;
    top: 50%;
    transform: translateY(-50%);
}

.sz-resultImg image {
    width: 112rpx;
    height: 96rpx;
}

.outer {
    display: flex;
    justify-content: center;
    align-items: center;
    width: 100%;
    height: 100%;
    perspective: 500rpx;
    perspective-origin: 50% 50%;
    overflow: hidden;
    transform: scale(.9);
}

.group {
    width: 200rpx;
    height: 200rpx;
    transform-style: preserve-3d;
    transform-origin: 50% 50% -100rpx;
    transform: rotateY(0deg) rotateX(90deg) rotateZ(36deg);
    background-color: #e8e8e9;
    border-radius: 20rpx;
}

.group.active1 {
    animation: scroll1 .8s linear 0s forwards;
}

.group.active2 {
    animation: scroll2 .8s linear 0s forwards;
}

.group.active3 {
    animation: scroll3 .8s linear 0s forwards;
}

.group.active4 {
    animation: scroll4 .8s linear 0s forwards;
}

.group.active5 {
    animation: scroll5 .8s linear 0s forwards;
}

.group.active6 {
    animation: scroll6 .8s linear 0s forwards;
}

.page {
    width: 200rpx;
    height: 200rpx;
    padding: 0!important;
    position: absolute;
    border-radius: 20rpx;
    font-weight: 700;
    overflow: hidden;
    word-break: break-all;
    word-wrap: break-word;
    text-align: center;
    background-color: #e8e8e9;
}

.page2 {
    transform-origin: right;
    transform: rotateY(-90deg);
}

.page3 {
    transform-origin: left;
    transform: rotateY(90deg);
}

.page4 {
    transform-origin: top;
    transform: rotateX(-90deg);
}

.page5 {
    transform-origin: bottom;
    transform: rotateX(90deg);
}

.page6 {
    transform: translateZ(-200rpx);
}

.page .dot {
    display: inline-block;
    width: 50rpx;
    height: 50rpx;
    border-radius: 25rpx;
    background-color: #050505;
}

.page1 .dot {
    position: absolute;
    left: 50%;
    top: 50%;
    margin-top: -25rpx;
    margin-left: -25rpx;
    background-color: #bc3132;
}

.sz-red .page {
    background: #bc3132;
}

.sz-red .dot,.sz-red .page1 .dot {
    background: #fff;
}

.page2 .dot {
    margin: 75rpx 25rpx;
}

.page3 .dot {
    position: absolute;
}

.page3 .dot:first-child {
    top: 20rpx;
    left: 20rpx;
}

.page3 .dot:nth-child(2) {
    top: 75rpx;
    left: 75rpx;
}

.page3 .dot:last-child {
    bottom: 20rpx;
    right: 20rpx;
}

.page4 .dot,.page5 .dot {
    margin: 25rpx;
}

.page5 .dot:last-child {
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -25rpx;
    margin-top: -25rpx;
}

.page6 .dot {
    margin: 12rpx 25rpx;
    width: 35rpx;
    height: 35rpx;
    border-radius: 20rpx;
}

@-webkit-keyframes scroll1 {
    0% {
        transform: rotateY(0deg) rotateX(40deg) rotateZ(0deg);
    }

    99% {
        transform: rotateY(4turn) rotateX(40deg) rotateZ(50deg);
    }

    to {
        transform: rotateY(0deg) rotateX(0) rotateZ(0);
    }
}

@keyframes scroll1 {
    0% {
        transform: rotateY(0deg) rotateX(40deg) rotateZ(0deg);
    }

    99% {
        transform: rotateY(4turn) rotateX(40deg) rotateZ(50deg);
    }

    to {
        transform: rotateY(0deg) rotateX(0) rotateZ(0);
    }
}

