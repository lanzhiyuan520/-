.ct-img {
    width: 120rpx;
    height: 120rpx;
    padding: 10rpx 10rpx 0 0;
    line-height: 120rpx;
    margin: 8rpx;
    display: inline-block;
    position: relative;
    box-sizing: border-box;
    font-size: 34rpx;
    font-weight: 550;
    text-align: center;
    text-decoration: none;
    -webkit-tap-highlight-color: transparent;
    overflow: hidden;
    color: #fff;
    background: url("data:image/png;base64,") 0 0 no-repeat;
    background-size: 100% 100%;
}

.bgc-skyblue {
    background-color: #87ceeb;
}

.bgc-yellowgreen {
    background-color: #9acd32;
}

.bgc-pink {
    background-color: pink;
}

.ct-view {
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
}

.main,.result-view {
    display: flex;
    flex-direction: column;
}

.result-view {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    justify-content: center;
    align-items: center;
    background-color: #fff;
}

.result-img {
    width: 90%;
}

.result-num {
    width: 200rpx;
    height: 300rpx;
    line-height: 300rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 80pt;
    color: red;
}

.restart-btn {
    margin-top: 30rpx;
    width: 50%;
}

.sw-tips {
    text-align: center;
    margin-top: 8rpx;
}

.ad-bottom {
    bottom: 0;
}

.ad-bottom,.ad-bottom-hidden {
    left: 0;
    right: 0;
    position: fixed;
    margin: 0 auto;
}

.ad-bottom-hidden {
    bottom: 1;
}

.ad-view {
    position: fixed;
    left: 5%;
    bottom: 0;
    width: 90%;
}

.con {
    position: relative;
    padding: 20rpx 0;
}

.back {
    position: absolute;
    top: 0;
    width: 100%;
    height: 100%;
}