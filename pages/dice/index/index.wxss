.wrap {
    width: 100%;
    height: 100%;
}

#dicecup {
    position: absolute;
    width: 600rpx;
    height: 700rpx;
    top: 51%;
    left: 50%;
    margin-left: -300rpx;
    margin-top: -160rpx;
}

#dicecup-bottom,.dicecup-top {
    width: 660rpx;
    height: 990rpx;
    position: absolute;
    bottom: 100rpx;
    left: 50%;
    filter: hue-rotate(130deg);
    margin-left: -330rpx;
}

#dice-container {
    width: 600rpx;
    height: 300rpx;
    position: absolute;
    bottom: 170rpx;
    left: 50%;
    margin-left: -300rpx;
    transform: scale(.95);
}

.dice {
    position: absolute;
    width: 110rpx;
    height: 110rpx;
}

#dice1 {
    top: 30rpx;
    left: 200rpx;
}

#dice2 {
    top: 35rpx;
    left: 300rpx;
}

#dice3 {
    top: 60rpx;
    left: 100rpx;
}

#dice4 {
    top: 100rpx;
    left: 210rpx;
}

#dice5 {
    top: 70rpx;
